<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.kegafirstapp.testxml.MainActivity">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:hint="Large Text"
        android:id="@+id/textView_ksoap"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="91dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:hint="Large Text"
        android:id="@+id/textView2"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView_ksoap"
        android:layout_alignParentLeft="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:hint="Large Text"
        android:id="@+id/textView3"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView2"
        android:layout_alignParentLeft="true"
        android:text="3" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Press The Button"
        android:id="@+id/btnView"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TestConnect"
        android:id="@+id/btnToJdom"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Temperature"
        android:id="@+id/bTemp"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sql Connect"
        android:id="@+id/bSqlConnect"
        android:layout_alignTop="@+id/bTemp"
        android:layout_alignLeft="@+id/btnToJdom"
        android:layout_alignStart="@+id/btnToJdom"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Custumer"
        android:id="@+id/bCustomer"
        android:layout_below="@+id/bTemp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/bTemp"
        android:layout_alignEnd="@+id/bTemp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TestChart"
        android:id="@+id/bChart"
        android:layout_alignTop="@+id/bCustomer"
        android:layout_alignLeft="@+id/bSqlConnect"
        android:layout_alignStart="@+id/bSqlConnect"
        android:layout_alignRight="@+id/btnToJdom"
        android:layout_alignEnd="@+id/btnToJdom" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Line Chart"
        android:id="@+id/bLineChart"
        android:layout_below="@+id/bCustomer"
        android:layout_alignRight="@+id/bCustomer"
        android:layout_alignEnd="@+id/bCustomer"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Quarter Chart"
        android:id="@+id/bQChart"
        android:layout_alignTop="@+id/bLineChart"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignLeft="@+id/bChart"
        android:layout_alignStart="@+id/bChart" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="QR code "
        android:id="@+id/bQRCode"
        android:layout_below="@+id/bQChart"
        android:layout_alignRight="@+id/bLineChart"
        android:layout_alignEnd="@+id/bLineChart"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Top Cust."
        android:id="@+id/bTopCust"
        android:layout_below="@+id/bQChart"
        android:layout_alignLeft="@+id/bQChart"
        android:layout_alignStart="@+id/bQChart"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />
</RelativeLayout>
