<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kegafirstapp.testxml">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-feature android:name="android.hardware.camera" android:required="false" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ShowParser"
            android:label="showparser">
        </activity>

        <activity
            android:name=".Ksoap"
            android:label="ksoap">
        </activity>

        <activity
            android:name=".Temperature"
            android:label="temperature">
        </activity>

        <activity
            android:name=".SqlConnect"
            android:label="sqlconnect">
        </activity>

        <activity
            android:name=".CustomerList"
            android:label="customerlist">
        </activity>

        <activity
            android:name=".EditCustomer"
            android:label="editcustomer">
        </activity>

        <activity
            android:name=".TestChart"
            android:label="testchart">
        </activity>

        <activity
            android:name=".Chart_Line"
            android:label="chart_line">
        </activity>

        <activity
            android:name=".SelectQuarter"
            android:label="selectquarter">
        </activity>

        <activity
            android:name=".QuarterChart"
            android:label="quarterchart">
        </activity>

        <activity
            android:name=".TestQrCode"
            android:label="testqrcode">
        </activity>

        <activity
            android:name=".TopCustomerChart"
            android:label="topcustomerchart">
        </activity>

        <activity
            android:name=".SalesLineChart"
            android:label="saleslinechart">
        </activity>

    </application>

</manifest>